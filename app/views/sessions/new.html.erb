<h1>Sign In Bitches</h1>

<%= form_for :user, url: session_url, method: 'POST' do |f| %>
  <div>
    <%= f.label :username %>
    <%= f.text_field :username %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit 'Sign In' %>
  </div>
<% end %>
